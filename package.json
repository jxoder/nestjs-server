{
  "$schema": "https://json.schemastore.org/package",
  "name": "nestjs-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:dev": "nest start --watch",
    "build": "nest build",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "jest": "ENV=test node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "infra": "docker-compose -f res/docker-compose.yml up",
    "migration:new": "pnpm typeorm migration:create migrations/migration",
    "migration:gen": "pnpm typeorm migration:generate migrations/migration -p -d ormconfig.ts",
    "migration:run": "pnpm typeorm migration:run -d",
    "migration:revert": "pnpm typeorm migration:revert -d"
  },
  "dependencies": {
    "@nestjs/common": "11.0.5",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.5",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.5",
    "@nestjs/swagger": "11.0.3",
    "@nestjs/typeorm": "11.0.0",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "compression": "1.7.5",
    "dayjs": "1.11.13",
    "env-var": "7.5.0",
    "express": "4.21.2",
    "jsonwebtoken": "9.0.2",
    "passport": "0.7.0",
    "passport-custom": "1.1.1",
    "pg": "8.13.1",
    "request-ip": "3.3.0",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "4.1.0",
    "typeorm-transactional": "0.5.0",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@electric-sql/pglite": "0.2.16",
    "@eslint/eslintrc": "3.2.0",
    "@nestjs/cli": "11.0.2",
    "@nestjs/testing": "11.0.5",
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.8",
    "@types/node": "22.10.10",
    "@types/request-ip": "0.0.41",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.21.0",
    "@typescript-eslint/parser": "8.22.0",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "jiti": "2.4.2",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "supertest": "7.0.0",
    "timekeeper": "2.3.1",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typeorm-pglite": "0.3.2",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "bcrypt"
    ]
  }
}
